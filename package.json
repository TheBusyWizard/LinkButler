{
	"name": "LinkButler",
	"version": "2.0.0",
	"description": "A tool for managing (searching, sorting, tagging, etc) very large collections of URLs and their associated resources.",
	"repository": "https://thebusywizard.dev/git/TheBusyWizard/LinkButler",
	"author": "TheBusyWizard <the.busy.wizard@gmail.com>",
	"license": "MIT",
	"scripts": {
		"build": "parcel build --no-autoinstall --no-hmr --cache-dir _cache/parcel --dist-dir _build source/index.html",
		"serve": "parcel serve --no-autoinstall --no-hmr --cache-dir _cache/parcel --dist-dir _build source/index.html"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.16",
		"@babel/core": "^7.16",
		"@babel/preset-env": "^7.16",
		"@parcel/core": "^2.1",
		"@parcel/packager-raw-url": "^2.0",
		"@parcel/transformer-sass": "^2.0",
		"@parcel/transformer-webmanifest": "^2.0",
		"parcel": "^2.0"
	},
	"dependencies": {}
}
